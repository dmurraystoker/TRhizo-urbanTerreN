	issn = {0028-646X, 1469-8137},
	journal = {New Phytologist},
	language = {en},
	month = oct,
	number = {2},
	pages = {211--237},
	title = {Developmental biology of legume nodulation},
	url = {http://doi.wiley.com/10.1111/j.1469-8137.1992.tb04227.x},
	urldate = {2020-07-23},
	volume = {122},
	year = {1992},
	Bdsk-Url-1 = {http://doi.wiley.com/10.1111/j.1469-8137.1992.tb04227.x},
