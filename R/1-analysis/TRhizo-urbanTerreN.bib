	number = {5},
	pages = {291--303},
	shorttitle = {Rhizobia},
	title = {Rhizobia: from saprophytes to endosymbionts},
	url = {http://www.nature.com/articles/nrmicro.2017.171},
	urldate = {2020-07-23},
